# MONGODB SIMPLE

## local develop
DDB_ATLAS=false
DDB_HOST=0.0.0.0
DDB_PORT=27017
DDB_NAME=delii
DDB_USER=
DDB_PASSWORD=
DDB_CERT=false
DDB_CERT_DEV=false

# SERVER API
# dev = develop, test = testing, release= produccion
PORT=9000
ROLES="SUPERADMIN_ROL,ADMIN_ROL,EDITOR_ROL,USER_ROL,PROV_ROL,AGENT1_ROL"

# LOGS settings:
# LOGS_FILE: true | false (default: false)
# Set it to true and take look for "logs" folder inside this project.
LOGS_FILE=false
# LOGS_FORMAT: dev | combined | common | short | tiny
# (default: dev | recommended: combined)
LOGS_FORMAT=dev
# docs: https://github.com/expressjs/morgan#predefined-formats

# BCRYPT
SALT_ROUNDS=10

# MAILER
USER_AUTH_MAIL=
PASS_AUTH_MAIL=
HOST_MAIL=
PORT_MAIL=
# emails to BCC when sending the invoice separated by comma
BCC=

# AWS S3 OR SIMILAR
AWS_ID=ID_AWS
AWS_KEY=KEY_AWS
## You must remove the last '/'. Must end only with '.com'
AWS_S3_BUCKET=my-bucket
AWS_S3_ENDPOINT=https://my-bucket.nyc3.digitaloceanspaces.com
AWS_S3_ENDPOINT_CONNECTION=https://nyc3.digitaloceanspaces.com

# REDIS
