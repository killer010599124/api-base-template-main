    "@socket.io/redis-adapter": "8.1.0",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "^4.18.2",
    "express-graphql": "^0.12.0",
    "generate-password": "1.7.0",
    "graphql": "^15.8.0",
    "graphql-tag": "^2.12.6",
    "http": "0.0.1-security",
    "jsonwebtoken": "9.0.0",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "moment-timezone": "0.5.42",
    "mongoose": "^7.0.3",
    "morgan": "1.10.0",
    "node-cron": "3.0.2",
    "nodemailer": "6.9.1",
