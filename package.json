
{
  "name": "api-base-template",
  "version": "0.0.1",
  "description": "Delii API Template",
  "scripts": {
    "start": "NODE_ENV=production node dist/server.js",
    "start-dev": "NODE_ENV=development node dist/server.js",
  
    "dev": "NODE_ENV=development nodemon src/server.js --exec babel-node",
    "lint": "eslint ./ --fix",
    "prettier": "prettier -c ./ -w",
    "build": "rimraf dist && babel src -d dist",
    "docs": "rimraf public/apidoc && apidoc -i Docs -o public/apidoc/",
    "precommit": "npm run lint && npm run prettier && npm run build && npm run docs",
    "test": "NODE_ENV=test jest --detectOpenHandles",
    "coverage": "jest --coverage --runInBand --verbose --detectOpenHandles",
    "migration": "NODE_ENV=development babel-node Migrations/GeneralMigration.js",
    "masive-migration-dev": "NODE_ENV=development babel-node Migrations/CustomMigration.js",
    "masive-migration": "NODE_ENV=production babel-node Migrations/CustomMigration.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:DeliiApp/template.git"
  },
  "keywords": [],
  "author": "velantcode",
  "license": "ISC",
  "homepage": "https://github.com/DeliiApp/api-base-template#readme",
  "devDependencies": {
    "@babel/cli": "7.21.0",
    "@babel/core": "7.21.3",
    "@babel/node": "7.20.7",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.20.2",
    "@babel/register": "7.21.0",
    "@faker-js/faker": "7.6.0",
    "@types/request": "2.48.8",
    "apidoc": "0.54.0",
    "babel-cli": "6.26.0",
    "babel-jest": "29.5.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "eslint": "8.37.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-unused-imports": "2.0.0",
    "nodemon": "^2.0.22",
    "prettier": "2.8.7",
    "random-coordinates": "1.0.1",
    "rimraf": "4.4.1"
  },
  "dependencies": {
    "@apollo/server": "^4.7.1",
    "@aws-sdk/client-s3": "3.303.0",
    "@sentry/node": "7.46.0",
